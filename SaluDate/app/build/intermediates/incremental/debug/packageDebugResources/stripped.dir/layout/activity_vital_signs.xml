<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="8dp"
    tools:context=".VitalSigns">

    <TextView
        android:id="@+id/textView2_e"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:fontFamily="@font/raleway_semibold"
        android:text="@string/vital_signs_e"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/divider2"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:layout_marginTop="12dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2_e" />

    <ImageButton
        android:id="@+id/butt_sugarBlood_e"
        android:layout_width="148dp"
        android:layout_height="148dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="15dp"
        android:background="#FFFFFF"
        android:onClick="onClik"
        android:padding="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider2"
        app:srcCompat="@drawable/sugarblood" />

    <ImageButton
        android:id="@+id/butt_temperature_e"
        android:layout_width="145dp"
        android:layout_height="145dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="28dp"
        android:background="#FFFFFF"
        android:onClick="onClik"
        android:padding="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider2"
        app:srcCompat="@drawable/temperature" />

    <ImageButton
        android:id="@+id/butt_heartRate_e"
        android:layout_width="148dp"
        android:layout_height="148dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="12dp"
        android:background="#FFFFFF"
        android:onClick="onClik"
        android:padding="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/butt_sugarBlood_e"
        app:srcCompat="@drawable/heartrate" />

    <ImageButton
        android:id="@+id/butt_bloodPreasure_e"
        android:layout_width="148dp"
        android:layout_height="148dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="28dp"
        android:background="@android:color/transparent"
        android:onClick="onClik"
        android:padding="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/butt_temperature_e"
        app:srcCompat="@drawable/bloodpresure" />

    <ImageButton
        android:id="@+id/butt_breathingFrequency_e"
        android:layout_width="148dp"
        android:layout_height="148dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="12dp"
        android:background="#FFFFFF"
        android:onClick="onClik"
        android:padding="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/butt_heartRate_e"
        app:srcCompat="@drawable/breathingfrecuency" />

    <ImageButton
        android:id="@+id/butt_oxygenation_e"
        android:layout_width="148dp"
        android:layout_height="148dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="28dp"
        android:background="#FFFFFF"
        android:onClick="onClik"
        android:padding="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/butt_bloodPreasure_e"
        app:srcCompat="@drawable/oxygenation" />

    <ImageButton
        android:id="@+id/butt_healtCondition_e"
        android:layout_width="148dp"
        android:layout_height="148dp"
        android:layout_marginStart="131dp"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="4dp"
        android:background="#FFFFFF"
        android:onClick="onClik"
        android:padding="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/healtcondition" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="41dp"
        android:layout_height="41dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back" />

</androidx.constraintlayout.widget.ConstraintLayout>